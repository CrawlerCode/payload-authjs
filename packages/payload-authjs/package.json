{
  "name": "payload-authjs",
  "version": "0.8.2",
  "type": "module",
  "description": "A Payload CMS 3 plugin for Auth.js 5",
  "repository": {
    "type": "git",
    "url": "https://github.com/CrawlerCode/payload-authjs.git",
    "directory": "packages/payload-authjs"
  },
  "author": {
    "name": "CrawlerCode",
    "email": "crawlercode@outlook.de",
    "url": "https://github.com/CrawlerCode"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js",
      "default": "./dist/client.js"
    },
    "./middleware": {
      "types": "./dist/payload/middleware.d.ts",
      "import": "./dist/payload/middleware.js",
      "default": "./dist/payload/middleware.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.js",
      "default": "./dist/components/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "payload",
    "cms",
    "payloadcms",
    "plugin",
    "typescript",
    "nextjs",
    "react",
    "auth.js",
    "next-auth"
  ],
  "scripts": {},
  "dependencies": {
    "@payloadcms/ui": "^3.16.0"
  },
  "devDependencies": {
    "@types/react": "19.1.3",
    "next": "15.3.2",
    "next-auth": "5.0.0-beta.28",
    "payload": "3.37.0",
    "react": "19.1.0",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "next": "^15.0.0",
    "next-auth": "^5.0.0-beta.25",
    "payload": "^3.16.0"
  }
}
