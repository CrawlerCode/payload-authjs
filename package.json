{
  "name": "payload-authjs",
  "version": "0.3.4",
  "type": "module",
  "description": "A Payload CMS 3 plugin for Auth.js 5",
  "repository": "git@github.com:CrawlerCode/payload-authjs.git",
  "author": "crawlercode@outlook.de",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./middleware": {
      "types": "./dist/payload/middleware.d.ts",
      "import": "./dist/payload/middleware.js",
      "default": "./dist/payload/middleware.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.js",
      "default": "./dist/components/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "payload",
    "cms",
    "payloadcms",
    "plugin",
    "typescript",
    "react",
    "auth.js",
    "next-auth"
  ],
  "scripts": {
    "dev": "nx --project payload-authjs-dev run dev",
    "build": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "peerDependencies": {
    "@payloadcms/next": "^3.0.0",
    "@payloadcms/ui": "^3.0.0",
    "next": "^15.0.0",
    "next-auth": "^5.0.0-beta.25",
    "payload": "^3.0.0"
  },
  "devDependencies": {
    "@nx/eslint": "20.0.6",
    "@nx/js": "20.0.6",
    "@nx/next": "20.0.6",
    "@payloadcms/eslint-config": "3.0.0",
    "@types/react": "npm:types-react@19.0.0-rc.1",
    "next": "15.0.3",
    "next-auth": "5.0.0-beta.25",
    "nx": "20.0.6",
    "payload": "3.0.0",
    "prettier": "^3.3.3",
    "react": "19.0.0-rc-02c0e824-20241028",
    "react-dom": "19.0.0-rc-02c0e824-20241028",
    "typescript": "^5.6.3"
  },
  "nx": {},
  "dependencies": {
    "@auth/core": "^0.37.2"
  },
  "packageManager": "pnpm@9.12.3",
  "pnpm": {
    "overrides": {
      "@types/react": "npm:types-react@19.0.0-rc.1"
    }
  }
}
